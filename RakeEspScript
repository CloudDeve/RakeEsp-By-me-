local a=game.Players.LocalPlayer;local b=a:GetMouse()local c=false;local d=true;b.KeyUp:Connect(function(e)if e=='m'then if c==false then c=true;d=false;while d==false do wait(0.3)if game.Workspace:FindFirstChild('Rake')then if not game.Workspace.Rake:FindFirstChild('Highlight')then local f=Instance.new('Highlight')f.Parent=game.Workspace.Rake;local g=Instance.new('BillboardGui')g.Parent=game.Workspace.Rake;g.Adornee=game.Workspace.Rake;g.StudsOffset=Vector3.new(0,4,0)g.Size=UDim2.new(0,200,0,50)g.AlwaysOnTop=true;local h=Instance.new('TextLabel')h.Parent=g;h.Size=UDim2.new(0,200,0,50)local i=coroutine.create(function()while wait(0.1)do if game.Workspace:FindFirstChild('Rake')then if game.Workspace.Rake:FindFirstChild('BillboardGui')then local j=(a.Character:FindFirstChild('HumanoidRootPart').Position-game.Workspace.Rake:FindFirstChild('HumanoidRootPart').Position).Magnitude;h.Text='Rake|'..'Health:'..math.floor(game.Workspace.Rake:FindFirstChildOfClass('Humanoid').Health)..'|Studs:'..math.floor(j)end end end end)coroutine.resume(i)h.TextScaled=true;h.TextColor3=Color3.fromRGB(255,255,255)h.BackgroundTransparency=1 end end end else c=false;d=true;while d==true do wait(0.3)if game.Workspace:FindFirstChild('Rake')then if game.Workspace.Rake:FindFirstChild('Highlight')then game.Workspace.Rake:FindFirstChild('Highlight'):Destroy()game.Workspace.Rake:FindFirstChild('BillboardGui'):Destroy()end end end end end end)
